        document.addEventListener('WeixinJSBridgeReady', function onBridgeReady() {
            window.shareData = {
                "imgUrl": "http://www.baidu.com",
                "timeLineLink": "http://www.baidu.com",
                "sendFriendLink": "http://www.baidu.com",
                "weiboLink": "http://www.baidu.com",
